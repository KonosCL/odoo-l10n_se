<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

        <record model="ir.ui.view" id="import_form_view">
            <field name="name">import.form</field>
            <field name="model">account.export.import</field>
            <field name="arch" type="xml">
                <form string="Load XML-file">
                    <group>
                        <field name="data"/>
                    </group>
                    <footer>
                        <button name="load_xml" string="_Import" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                    
                    
                </form>
            </field>
        </record>

        <act_window id="launch_import"
                    name="Load XML-file"
                    src_model="account.export"
                    res_model="account.export.import"
                    view_mode="form"
                    target="new"
                    key2="client_action_multi"/>
                    
        <record id="action_import_form_view" model="ir.actions.act_window">
            <field name="name">Import XML-file</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.export.import</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
                    
      <menuitem
        action="action_import_form_view"
        id="menu_view_base_import_language"
        parent="account.menu_finance_configuration"/>

  </data>
</openerp>

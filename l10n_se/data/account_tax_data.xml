<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="chart1955" model="account.account.template">
      <field name="name">Bank transfer</field>
      <field name="code">1955</field>
      <field name="user_type_id" ref="account.data_account_type_current_assets"/>
      <field name="reconsile" eval="True"/>
    </record>
    <!-- General chart_template -->
    <record id="chart_template_general" model="account.chart.template">
      <field name="name">General</field>
      <field name="transfer_account_id" ref="chart1955"/>
      <field name="currency_id" ref="base.SEK"/>
      <field name="cash_account_code_prefix">1910</field>
      <field name="bank_account_code_prefix">1930</field>
      <field name="code_digits">4</field>
      <field name="visible" eval="False"/>
    </record>
    
    <record id="chart1955" model="account.account.template">
            <field name="chart_template_id" ref="chart_template_general"/>
    </record>
    <!-- Account Tags -->
    <record id="tag_g" model="account.account.tag">
      <field name="name">G. Moms att betala eller få tillbaka.</field>
      <field name="applicability">taxes</field>
    </record>
    <record id="tag_49" model="account.account.tag">
      <field name="name">49. Moms att betala ut eller få tillbaka.</field>
      <field name="applicability">taxes</field>
    </record>
    <record id="tag_a" model="account.account.tag">
      <field name="name">A. Momspliktig försäljning eller uttag exklusive moms.</field>
      <field name="applicability">taxes</field>
    </record>    
    <record id="tag_5" model="account.account.tag">
      <field name="name">5. Momspliktig försäljning som inte ingår i annan ruta nedan</field>
      <field name="applicability">taxes</field>
    </record>    
    
